{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src"]
}
